package contabancaria;


public class NewClass {
    public static void main(String[]args){
        ContaBancaria Maria;
        Maria = new ContaBancaria();
        Maria.numero=(5678);
        Maria.cliente= "Maria Fernanda Camargo";
        Maria.saldo= (1000);
        Maria.limite=(2000);
        Maria.depositar(500);
        Maria.sacar(200);
        Maria.Quant_parcelas=(5);
        Maria.emprestar(200);
    }
    
}
